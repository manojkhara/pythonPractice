   "source": [
    "def DeleteMiddle(stack,k):\n",
    "    if k==1:\n",
    "        stack.pop()\n",
    "        return\n",
    "    x = stack.pop()\n",
    "    DeleteMiddle(stack,k-1)\n",
    "    stack.append(x)\n",
    "    \n",
    "stack = [1,2,3,4,6]\n",
    "k = (len(stack)//2)+1\n",
    "DeleteMiddle(stack,k)\n",
    "\n",
    "print(stack)"
   ]
